<ul class="comment-title">
    <li class="comment-title"><h1>Patients' Comments</h1></li>
</ul>

<HR class="comment-line">

<table class="comment-info">
    {{#each data}}
    <tr>
        <td class="comment-nav-left"><a href="/clinician/{{this.id}}">{{this.first_name}} {{this.last_name}}</a></td>
        <td><a href="/clinician/{{this.id}}">{{this.cmt}}</a></td>
        <td class="comment-nav-right">{{this.time}}</td>
        <!-- <img src="images/reply_icon.png" alt="logo"> -->
    </tr>
    {{/each}}

</table>